{
  "directories": ["src/", "templates/", "vendor/"],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        ".gitignore",
        ".md",
        "phpunit",
        "Tester",
        "Tests",
        "tests",
        "yaml"
      ],
      "in": "vendor"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Herrera\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "git-version": "app_version",
  "main": "spaceport",
  "output": "out/spaceport.phar",
  "stub": true,
  "chmod": "0755"
}
